{
  "name": "owl-twitch-bot",
  "version": "0.1.0",
  "description": "Chatbot that connects to twitch chat, can respond to commands with text and supplies the chat messages to other bundles",
  "homepage": "https://bitowl.net",
  "author": {
    "name": "bitowl",
    "email": "bitowl@bitowl.de",
    "url": "https://bitowl.net"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "twitch",
    "chat",
    "bot",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "bundleDependencies": {
      "owl-nodecg-styles": "^0.0.0"
    },
    "dashboardPanels": [
      {
        "name": "bot-commands",
        "title": "Bot Commands",
        "width": 3,
        "file": "bot-commands.html",
        "headerColor": "#4b367c"
      },
      {
        "name": "delete-command",
        "title": "Delete Command?",
        "width": 2,
        "file": "delete-command.html",
        "dialog": true,
        "dialogButtons": [
          {
            "name": "Yes",
            "type": "confirm"
          },
          {
            "name": "No",
            "type": "dismiss"
          }
        ]
      }
    ]
  },
  "dependencies": {
    "tmi.js": "^1.4.5",
    "twitch-bot": "^1.3.5"
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT"
}
